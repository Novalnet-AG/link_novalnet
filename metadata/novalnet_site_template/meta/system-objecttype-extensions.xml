    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="nnPublicKey">
                <display-name xml:lang="x-default">Product activation key</display-name>
                <description xml:lang="x-default">Get your Product activation key from the Novalnet Admin Portal&#58; PROJECT &#62; Choose your project &#62; Shop Parameters &#62; API Signature &#40;Product activation key&#41;</description>
                <type>string</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="nnPrivateKey">
                <display-name xml:lang="x-default">Payment access key</display-name>
                <description xml:lang="x-default"> Get your Payment access key from the Novalnet Admin Portal&#58; PROJECT &#62; Choose your project &#62; Shop Parameters &#62; Payment access key</description>
                <type>string</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="nnClientSecretKey">
                <display-name xml:lang="x-default">Client Key</display-name>
                <description xml:lang="x-default"> Get your client key from the Novalnet Admin Portal&#58; PROJECT &#62; Choose your project &#62; Shop Parameters &#62; Client Key</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="nnTariffID">
                <display-name xml:lang="x-default">Tariff ID</display-name>
