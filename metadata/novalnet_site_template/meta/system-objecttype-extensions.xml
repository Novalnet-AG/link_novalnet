                <type>string</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="nnEnabled">
                <display-name xml:lang="x-default">Novalnet Enabled</display-name>
                <description xml:lang="x-default">Enables or disables the cartridge</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="nnWebhookTestMode">
                <display-name xml:lang="x-default">Allow manual testing of the Notification &#47; Webhook URL</display-name>
                <description xml:lang="x-default">Enable this to test the Novalnet Notification &#47; Webhook URL manually. Disable this before setting your shop live to block unauthorized calls from external parties</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="nnWebhookEmailTo">
