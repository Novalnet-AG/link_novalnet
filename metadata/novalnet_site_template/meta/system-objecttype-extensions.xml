                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="nnWebhookUrl">
                <display-name xml:lang="x-default">Notification &#47; Webhook URL</display-name>
                <description xml:lang="x-default">You must configure the webhook endpoint in your Novalnet Admin portal. This will allow you to receive notifications about the transaction</description>
